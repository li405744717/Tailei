page{
    display: flex;
    flex-direction: column;
    min-height: 100%;
}
.option_item{
    margin-left: 30rpx;
    padding: 20rpx 0;
    margin-right: 30rpx;
    border-bottom: 1px solid #E4E4E4;
}
.select_container{
    border-radius: 20rpx 20rpx 0 0;
    background-color: white;
    width: 100%;
    height: 1130rpx;

}
.select_header_container{
    padding:28rpx 30rpx;
}
.select_content_container{
    background-color: #f4f4f4;
    padding:0 30rpx
}
.select_content_header{
    padding:28rpx 0;
    border-bottom: 1px solid #E4E4E4;
}
.city_item{
    padding:28rpx 0;
    border-bottom: 1px solid #E4E4E4;
}
.result_container{
    padding:28rpx 30rpx 0 28rpx;
}
.result_view_item{
    border-left: 1px solid #FFAB19;
    padding-left: 30rpx;
    position: relative;
    margin-right: 10rpx;
    overflow: visible;
    height: 66rpx;
}
.result_view_item_dot{
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background-color: #FFAB19;
    position: absolute;
    left: -10rpx;
    top: 0;
}
.result_view_item_un{
    border-left: 1px solid #D6D6D6;
    padding-left: 30rpx;
    position: relative;
    margin-right: 10rpx;
    overflow: visible;
    height: 66rpx;
}
.result_view_item_dot_un{
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background-color: #D6D6D6;
    position: absolute;
    left: -10rpx;
    top: 0;
}
.result_view_item_text{
    position: absolute;
    top:-10rpx
}